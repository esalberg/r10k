:cachedir: <%= @cachedir %>
<% unless @r10k_sources.empty? -%>
:sources:
<% @source_keys.sort.each do |source| -%>
  <%=source-%>:
<% @r10k_sources[source].sort.each do |key,value| -%>
    <%=key-%>: <%=value%>
<% end -%>
<% end -%>
<% end %>
<% unless @r10k_git.empty? -%>
:git:
<% @r10k_git.sort.each do |key,value| -%>
    <%=key-%>: <%=value%>
<% end -%>
<% end %>
